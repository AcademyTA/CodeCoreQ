<br>
<%= link_to 'My quizzes', quizzes_path %> <br><br>

<h1><%= @quiz.title %></h1>
<p>Quiz Body <%= @quiz.body %></p><br>
<p>Quiz Category <%= @category.title %></p><br>
<p>Quiz Level <%= @quiz.level %></p><br>

<%= link_to "Add questions", new_quiz_question_path(@quiz) %>
<hr>

<% @questions.each do |q| %>
  Question: <%= q.title %> <br>
  Body: <%= q.body %> <br>
    <% q.answers.each do |a| %>
      <%= a.body %><br>
    <% end %>

  <hr>
<% end %> 








