<%= link_to 'Add', new_quiz_question_path(@quiz) %>
<br><br>

<% @questions.each do |q| %>
  <%= q.title %><br>
  <%= q.body %><br>
  <%= q.explaination %><br>
  <%= link_to 'Edit', edit_quiz_question_path(@quiz, q) %>
  <%= link_to 'Delete', quiz_question_path(@quiz, q), method: :delete, data: {confirm: "Are you sure?"} %><br>  
<% end %> 